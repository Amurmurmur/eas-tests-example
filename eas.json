{
  "cli": {
    "version": ">= 0.60.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {},
    "test": {
      "ios": {
        "simulator": true,
        "buildArtifactPaths": ["artifacts/**/*.png"]
      }
    },
    "testDebug": {
      "ios": {
        "buildConfiguration": "Debug",
        "simulator": true,
        "buildArtifactPaths": ["artifacts/**/*.png"]
      }
    }
  },
  "submit": {
    "production": {}
  }
}
