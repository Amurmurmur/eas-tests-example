{
  "name": "eas-tests-example",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "detox:clean": "detox clean-framework-cache && detox build-framework-cache",
    "detox:ios:build": "detox build -c ios",
    "detox:ios:test": "detox test -c ios",
    "detox:android:build": "detox build -c android",
    "detox:android:test": "detox test -c android",
    "eas-build-pre-install": "./eas-hooks/eas-build-pre-install.sh",
    "eas-build-on-success": "./eas-hooks/eas-build-on-success.sh"
  },
  "dependencies": {
    "expo": "~46.0.7",
    "expo-dev-client": "~1.2.1",
    "expo-dev-launcher": "^1.2.1",
    "expo-dev-menu": "^1.2.1",
    "expo-splash-screen": "~0.16.1",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "~0.14.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.4",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "detox": "^19.9.3",
    "jest": "^28.1.3",
    "patch-package": "^6.4.7"
  },
  "private": true
}
